      "com.nokia.eda.timing.v1alpha1.NTPClient": {
        "description": "NTPClient is the Schema for the ntpclients API",
        "properties": {
          "apiVersion": {
            "default": "timing.eda.nokia.com/v1alpha1",
            "type": "string",
            "x-eda-nokia-com": {
              "ui-visible-if": "false"
            }
          },
          "kind": {
            "default": "NTPClient",
            "type": "string",
            "x-eda-nokia-com": {
              "ui-visible-if": "false"
            }
          },
          "metadata": {
            "$ref": "#/components/schemas/com.nokia.eda.timing.v1alpha1.NTPClient_metadata"
          },
          "spec": {
            "description": "The NTP client allows for configuring NTP servers and the source of NTP traffic in order for the devices to synchronize their clocks.",
            "properties": {
              "router": {
                "description": "Router used to reach the NTP servers.",
                "title": "Router",
                "type": "string",
                "x-eda-nokia-com": {
                  "ui-auto-completes": [
                    {
                      "condition": "self.routerKind==='DEFAULTROUTER'",
                      "group": "routing.eda.nokia.com",
                      "resource": "defaultrouters",
                      "type": "gvr",
                      "version": "v1alpha1"
                    },
                    {
                      "condition": "self.routerKind==='ROUTER'",
                      "group": "services.eda.nokia.com",
                      "resource": "routers",
                      "type": "gvr",
                      "version": "v1alpha1"
                    },
                    {
                      "condition": "self.routerKind === 'MANAGEMENTROUTER'",
                      "group": "bootstrap.eda.nokia.com",
                      "resource": "managementrouters",
                      "type": "gvr",
                      "version": "v1alpha1"
                    }
                  ],
                  "ui-column-span": 2,
                  "ui-conditions": [
                    {
                      "condition": "!((self.router === undefined || self.router === '') && (self.routerSelector === undefined || self.routerSelector.length === 0))",
                      "errorMsg": "Either Router or Router Selector must be provided"
                    }
                  ],
                  "ui-order-priority": 200,
                  "ui-single-line-group": "Router"
                }
              },
              "routerKind": {
                "description": "the Kind of the router used to reach the NTP servers.",
                "enum": [
                  "MANAGEMENTROUTER",
                  "ROUTER",
                  "DEFAULTROUTER"
                ],
                "title": "Router Kind",
                "type": "string",
                "x-eda-nokia-com": {
                  "ui-column-span": 2,
                  "ui-conditions": [
                    {
                      "condition": "!(router === undefined && routerSelector === undefined)",
                      "errorMsg": "Either Router or Router Selector must be provided"
                    }
                  ],
                  "ui-order-priority": 100,
                  "ui-single-line-group": "RouterKind"
                }
              },
              "routerSelector": {
                "description": "Selects router resources based on the defined KIND.  Applies to DefaultRouter only. Not supported for Router and ManagementRouter.",
                "format": "labelselector",
                "items": {
                  "type": "string"
                },
                "title": "Router Selector",
                "type": "array",
                "x-eda-nokia-com": {
                  "ui-column-span": 2,
                  "ui-conditions": [
                    {
                      "condition": "!((self.router === undefined || self.router === '') && (self.routerSelector === undefined || self.routerSelector.length === 0))",
                      "errorMsg": "Either Router or Router Selector must be provided"
                    }
                  ],
                  "ui-order-priority": 300,
                  "ui-single-line-group": "Router"
                }
              },
              "servers": {
                "description": "A list of NTP servers, each entry in the list can either be an IP address or an FQDN.",
                "items": {
                  "properties": {
                    "iBurst": {
                      "description": "Indicates whether this server should enable burst synchronization or not",
                      "title": "Enable iBurst",
                      "type": "boolean",
                      "x-eda-nokia-com": {
                        "ui-column-span": 4
                      }
                    },
                    "preferred": {
                      "description": "Indicates whether this server should be preferred or not",
                      "title": "Preferred Server",
                      "type": "boolean",
                      "x-eda-nokia-com": {
                        "ui-column-span": 4
                      }
                    },
                    "server": {
                      "description": "An NTP server can either be an IP address or an FQDN",
                      "title": "Server",
                      "type": "string",
                      "x-eda-nokia-com": {
                        "ui-column-span": 2
                      }
                    }
                  },
                  "required": [
                    "server"
                  ],
                  "type": "object"
                },
                "title": "Servers",
                "type": "array",
                "x-eda-nokia-com": {
                  "ui-order-priority": 600
                }
              },
              "sourceInterface": {
                "description": "Specifies a Interface resource to use as a source of NTP traffic. If none is specified the Node default behavior is used.",
                "title": "Source Interface",
                "type": "string",
                "x-eda-nokia-com": {
                  "ui-auto-completes": [
                    {
                      "condition": "self.sourceInterfaceKind==='DEFAULT'",
                      "group": "routing.eda.nokia.com",
                      "resource": "defaultinterfaces",
                      "type": "gvr",
                      "version": "v1alpha1"
                    },
                    {
                      "condition": "self.sourceInterfaceKind==='SYSTEM'",
                      "group": "routing.eda.nokia.com",
                      "resource": "systeminterfaces",
                      "type": "gvr",
                      "version": "v1alpha1"
                    },
                    {
                      "condition": "self.sourceInterfaceKind==='IRB'",
                      "group": "services.eda.nokia.com",
                      "resource": "irbinterfaces",
                      "type": "gvr",
                      "version": "v1alpha1"
                    },
                    {
                      "condition": "self.sourceInterfaceKind==='ROUTED'",
                      "group": "services.eda.nokia.com",
                      "resource": "routedinterfaces",
                      "type": "gvr",
                      "version": "v1alpha1"
                    }
                  ],
                  "ui-column-span": 2,
                  "ui-order-priority": 500,
                  "ui-single-line-group": "Interfaces"
                }
              },
              "sourceInterfaceKind": {
                "description": "Specifies the source interface Kind to use as a source of NTP traffic.",
                "enum": [
                  "IRB",
                  "ROUTED",
                  "DEFAULT",
                  "SYSTEM"
                ],
                "title": "Source Interface Kind",
                "type": "string",
                "x-eda-nokia-com": {
                  "ui-column-span": 2,
                  "ui-order-priority": 400,
                  "ui-single-line-group": "Interfaces"
                }
              }
            },
            "required": [
              "routerKind",
              "servers"
            ],
            "title": "Specification",
            "type": "object",
            "x-eda-nokia-com": {
              "ui-order-priority": 30
            }
          },
          "status": {
            "description": "NTPClientStatus defines the observed state of NTPClient",
            "properties": {
              "health": {
                "description": "Indicates the health score of the NTPClient",
                "title": "Health Score",
                "type": "integer"
              },
              "healthScoreReason": {
                "description": "Indicates the reason for the health score",
                "title": "Health Score Reason",
                "type": "string"
              },
              "lastChange": {
                "description": "The time when the state of the resource last changed",
                "format": "date",
                "title": "Last Change",
                "type": "string"
              },
              "nodes": {
                "description": "List of nodes which are not synchronized",
                "items": {
                  "properties": {
                    "node": {
                      "description": "Reference to Node object",
                      "title": "Node",
                      "type": "string"
                    },
                    "operatingSystem": {
                      "description": "Operating System of the Node",
                      "title": "Operating System",
                      "type": "string"
                    },
                    "synchronized": {
                      "description": "Synchronized state of the Node",
                      "title": "Synchronized",
                      "type": "string"
                    }
                  },
                  "required": [
                    "node",
                    "operatingSystem"
                  ],
                  "type": "object"
                },
                "title": "Unsynchronized Nodes",
                "type": "array"
              },
              "operationalState": {
                "description": "Operational state of the NTPClient",
                "title": "Operational State",
                "type": "string"
              }
            },
            "readOnly": true,
            "title": "Status",
            "type": "object",
            "x-eda-nokia-com": {
              "ui-order-priority": 20
            }
          }
        },
        "required": [
          "apiVersion",
          "kind",
          "metadata",
          "spec"
        ],
        "type": "object"
      },
